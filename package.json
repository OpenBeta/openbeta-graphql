    "uuid": "^8.3.2",
    "uuid-mongodb": "^2.5.1"
  },
  "scripts": {
    "lint": "ts-standard",
    "fix": "ts-standard --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "tsc  -p tsconfig.build.json",
