    "ts-standard": "^11.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@graphql-tools/schema": "^8.3.1",
    "@openbeta/sandbag": "^0.0.11",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/centroid": "^6.5.0",
    "@turf/circle": "^6.5.0",
    "@types/uuid": "^8.3.3",
    "algoliasearch": "^4.11.0",
    "apollo-datasource-mongodb": "^0.5.4",
    "apollo-server": "^3.5.0",
    "cors": "^2.8.5",
